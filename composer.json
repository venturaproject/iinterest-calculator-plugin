{
  "name": "interestcalculator/interest-calculator-plugin",
  "description": "Plugin para calcular intereses",
  "type": "wordpress-plugin",
  "autoload": {
    "psr-4": {
      "InterestCalculatorPlugin\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "composer/installers": "^1.10",
    "pimple/pimple": "^3.5"
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/interest-calculator": ["type:wordpress-plugin"]
    }
  },
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
